<%
   @order = 20
   new_pages = pages_created
   updated_pages = pages_updated
%>

<%= section 'Pages' %>

<%= subsection 'New & Modified' %>

<%= time_series_chart(
  {:label => 'new pages', :data => new_pages},
  {:label => 'modified pages', :data => updated_pages}
)%>

<%= subsection 'Totals' %>

<%= time_series_chart(
  {:label => 'total new pages', :data => cumulative(new_pages)},
  {:label => 'total modified pages', :data => cumulative(updated_pages)}
)%>

